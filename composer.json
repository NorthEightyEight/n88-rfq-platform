{
    "name": "n88-rfq-platform/pdf-extraction",
    "description": "PDF extraction dependencies for N88 RFQ Platform",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "smalot/pdfparser": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "N88RFQ\\": "includes/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:verbose": "phpunit --verbose",
        "test:coverage": "phpunit --coverage-html coverage/"
    }
}
